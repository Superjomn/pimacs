@template[T]
class List:
    var list: Lisp

    def __init__(self):
        self.list = %list()

    def append(self, v: T):
        %push(v, self.list)

    def __getitem__(self, idx: Int) -> T:
        return %nth(self.list, idx)

    def size(self) -> Int:
        return %length(self.list)

# CHECK: var a :List[Int] = List[Int]()
var a = List[Int]()
# CHECK-NEXT: a.append(1)
a.append(1)

