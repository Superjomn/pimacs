# forward reference
# perform:
#   - name binding
#   - function overloading
#   - type inference

# CHECK: var a :Int = foo()
# CHECK: var b :Float = foo(1)
var a = foo()
var b = foo(1)
let a-int = 1
let a-float = 1.0

# CHECK: var c :Float = foo(a-int, a-float)
var c = foo(a-int, a-float)


def foo() -> Int:
    return 1

def foo(a: Int) -> Float:
    return 1.0

def foo(a: Int, b :Float) -> Float:
    return a-float

class Pet:
    var name: Str
    var age: Int

# CHECK: var a-pet :Pet = Pet("hello", 1)
# CHECK-NEXT: var age-1 :Int = a-pet.age
var a-pet = Pet("hello", 1)
var age-1 = a-pet.age
